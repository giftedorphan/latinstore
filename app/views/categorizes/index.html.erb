<p id="notice"><%= notice %></p>

<h1>Categorizes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @categorizes.each do |categorize| %>
      <tr>
        <td><%= link_to 'Show', categorize %></td>
        <td><%= link_to 'Edit', edit_categorize_path(categorize) %></td>
        <td><%= link_to 'Destroy', categorize, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Categorize', new_categorize_path %>
