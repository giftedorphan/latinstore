  <div class="wrapper">
  <div class="col-md-12">
    <h1 class="first_title">
      <b><%= @tag.name %></b>
    </h1>
  </div>

  <div class="col-md-12 space">
    <div class="col-md-6">
      <div class="col-md-12">
        <p class="label-tag"> Name: </p>
      </div>
      <div class="col-md-12">
        <p class="data"> <%= @tag.name %> </p>
      </div>
    </div>
  </div>

  <div class="col-md-4 col-md-push-2 center-div space">
    <%= link_to 'Editar', edit_tag_path(:id => @tag.id), {class: 'btn btn-primary btn-block'} %>
  </div>

  <div class="col-md-4 col-md-push-2 center-div space">
    <%= link_to 'Volver', tags_path, {class: 'btn btn-primary btn-block'} %>
  </div>
</div>